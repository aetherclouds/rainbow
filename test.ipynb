    "import torch\n",
    "from torchviz import make_dot\n",
    "x=torch.ones(2, requires_grad=True)\n",
    "y=2*x\n",
    "z=(3+x)\n",
    "r=(y+z).sum()\n",
    "make_dot(r)\n",
    "\n"
   ]
